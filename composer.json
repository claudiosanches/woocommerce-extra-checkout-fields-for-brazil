{
	"name": "claudiosanches/brazilian-market-on-woocommerce",
	"description": "Brazilian Market on WooCommerce.",
	"homepage": "https://wordpress.org/plugins/woocommerce-extra-checkout-fields-for-brazil/",
	"type": "wordpress-plugin",
	"license": "GPL-3.0-or-later",
	"require": {
		"php": ">=5.6|>=7.0",
		"composer/installers": "1.6.0"
	},
	"require-dev": {
		"phpunit/phpunit": "7.5.15",
		"wp-coding-standards/wpcs": "2.1.1",
		"dealerdirect/phpcodesniffer-composer-installer": "0.5.0",
		"phpcompatibility/phpcompatibility-wp": "2.1.0"
	},
	"autoload": {
		"psr-4": {
			"ClaudioSanches\\BrazilianMarket\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ClaudioSanches\\BrazilianMarket\\Tests\\": "tests/php/"
		}
	},
	"scripts": {
		"test": [
			"phpunit"
		],
		"phpcs": [
			"phpcs -sp --standard=./phpcs.ruleset.xml"
		],
		"phpcs-pre-commit": [
			"phpcs -spn --standard=./phpcs.ruleset.xml"
		],
		"phpcbf": [
			"phpcbf -p"
		]
	},
	"extra": {
		"scripts-description": {
			"test": "Run unit tests",
			"phpcs": "Analyze code against the WordPress coding standards with PHP_CodeSniffer",
			"phpcbf": "Fix coding standards warnings/errors automatically with PHP Code Beautifier"
		}
	}
}